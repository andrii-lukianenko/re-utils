{
  "name": "@andrii_lukianenko/re-utils",
  "version": "0.0.1",
  "description": "Shared utilities used in frontend-platform",
  "type": "module",
  "exports": {
    "./helpers": {
      "import": {
        "types": "./lib/types/helpers/index.d.ts",
        "default": "./lib/helpers.js"
      },
      "require": {
        "types": "./lib/types/helpers/index.d.ts",
        "default": "./lib/helpers.cjs"
      }
    },
    "./hooks": {
      "import": {
        "types": "./lib/types/hooks/index.d.ts",
        "default": "./lib/hooks.js"
      },
      "require": {
        "types": "./lib/types/hooks/index.d.ts",
        "default": "./lib/hooks.cjs"
      }
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts"
  },
  "author": {
    "name": "Andrii Lukianenko",
    "email": "andrii.lukianenko@olx.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrii-lukianenko/re-utils.git"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    }
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^14.3.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^18.3.11",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.52.4",
    "rollup-plugin-delete": "^3.0.1",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.0",
    "typescript": "^5.9.3"
  }
}
